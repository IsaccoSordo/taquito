{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "accessToken": "placeholder",
        "useDaemonProcess": false
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "example": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "docusaurus": {
      "cache": true
    }
  },
  "affected": {
    "defaultBase": "master"
  }
}
